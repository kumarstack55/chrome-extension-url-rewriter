{
  "manifest_version": 3,
  "name": "Share link",
  "short_name": "Share link",
  "description": "Rewrite URLs based on defined rules for shareing links.",
  "version": "0.0.1",
  "homepage_url": "https://github.com/kumarstack55/chrome-extension-share-link",
  "permissions": ["activeTab"],
  "commands": {},
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
